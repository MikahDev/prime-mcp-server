{
  "name": "prime-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Prime Ecosystem API - Insurance/Construction job management",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "prime-mcp-server": "./dist/index.js",
    "prime-mcp": "./dist/index.js"
